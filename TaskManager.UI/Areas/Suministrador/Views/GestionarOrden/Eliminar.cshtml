@model OrdenAdquisicionDTO

<h1 class="text-center">¿Estás seguro/a de eliminar este orden?</h1>

<form asp-action="Eliminar" asp-controller="GestionarOrden" method="post" style="margin-top: 12%">
    <div class="col-lg-4">
        @Html.LabelFor(model => model.ProductoSuministrador.NombreProducto)
        @Html.TextBoxFor(model => model.ProductoSuministrador.NombreProducto, "", new { @class = "form-control", @disabled = true})
    </div>

    <div class="col-lg-4">
        @Html.LabelFor(model => model.PrecioTotal)
        @Html.TextBoxFor(model => model.PrecioTotal, "", new { @class = "form-control", @disabled = true })
    </div>

    <div class="col-lg-4">
        @Html.LabelFor(model => model.ProductoSuministrador.Suministrador.RazonSocial)
        @Html.TextBoxFor(model => model.ProductoSuministrador.Suministrador.RazonSocial, "", new { @class = "form-control", @disabled = true })
    </div>

    <div class="col-lg-4">
        @Html.LabelFor(model => model.CantidadDeOrden)
        @Html.TextBoxFor(model => model.CantidadDeOrden, "", new { @class = "form-control", @disabled = true })
    </div>

    <div class="col-lg-4">
        @Html.LabelFor(model => model.FechaDeCreacion)
        @Html.TextBoxFor(model => model.FechaDeCreacion, "", new { @class = "form-control", @disabled = true })
    </div>

    <div class="col-lg-4">
        @Html.LabelFor(model => model.Estado)
        @Html.TextBoxFor(model => model.Estado, "", new { @class = "form-control", @disabled = true })
    </div>

    <div class="col-lg-12">
        <button type="submit" class="btn btn-danger">Eliminar</button>
    </div>


</form>